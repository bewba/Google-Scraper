{
  "manifest_version": 2,
  "name": "Google Places Scraper",
  "version": "1.0",
  "description": "Scrapes visible Google Maps places and downloads as CSV",
  "permissions": ["activeTab", "tabs", "downloads"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Google Places Scraper"
  },
  "icons": { "48": "icon.png" }
}
